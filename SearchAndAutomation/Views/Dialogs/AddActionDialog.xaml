<ui:UiWindow x:Class="SearchAndAutomation.Views.Dialogs.AddActionDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
        prism:ViewModelLocator.AutoWireViewModel="True"
        xmlns:imageBase="clr-namespace:SearchAndAutomation.ViewModels.ImageHandler.Base"
        WindowStartupLocation="CenterScreen"
        Title="{Binding Title}" Height="400" Width="300" >

    <DockPanel>
        <StackPanel DockPanel.Dock="Top" Orientation="Horizontal">
            <ui:Button Margin="4" Icon="SelectObject24" Content="Mouse Position" Command="{Binding AddActionCommand}"></ui:Button>
            </StackPanel>
    </DockPanel>
</ui:UiWindow>
